    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/async-retry": "^1.4.8",
    "@types/google-protobuf": "^3.15.12",
    "@types/jest": "^29.5.12",
    "@types/json-stable-stringify": "^1.0.36",
    "@types/pluralize": "^0.0.33",
