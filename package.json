    "pino": "^8.19.0",
    "pluralize": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-linkify-it": "^1.0.8",
    "remix-utils": "^7.5.0",
    "use-local-storage-state": "^19.2.0",
    "use-previous": "^1.2.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
