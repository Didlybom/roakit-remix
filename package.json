{
  "name": "@roakit/remix-app",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "sideEffects": false,
  "scripts": {
    "artifactregistry:login": "npx google-artifactregistry-auth",
    "build": "remix build",
    "build.vite": "remix vite:build",
    "local": "cross-env remix dev --manual",
    "local.vite": "cross-env remix vite:dev --manual",
    "local.prod": "cross-env ROAKIT_ENV=production remix dev --manual",
    "deploy": "npm run build && firebase use default; firebase deploy --only hosting,functions:remixApp",
    "deploy.prod": "npm run build && firebase use prod; firebase deploy --only hosting,functions:remixApp",
    "start": "remix-serve ./functions/build/index.js",
    "typecheck": "tsc",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "jest",
    "svgr": "npx @svgr/cli --out-dir app/icons -- icons"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@google-cloud/secret-manager": "^5.2.0",
    "@mui/material": "^5.15.14",
    "@mui/x-charts": "^7.1.0",
    "@mui/x-data-grid": "^7.1.0",
    "@remix-run/css-bundle": "^2.8.1",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "@remix-run/server-runtime": "^2.8.1",
    "async-retry": "^1.3.3",
