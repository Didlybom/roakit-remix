{
  "name": "@roakit/remix-app",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "sideEffects": false,
  "scripts": {
    "artifactregistry:login": "npx google-artifactregistry-auth",
    "build": "remix build",
    "build.vite": "remix vite:build",
    "local": "cross-env remix dev --manual",
    "local.vite": "cross-env remix vite:dev --manual",
    "local.vite.prod": "cross-env ROAKIT_ENV=production remix vite:dev --manual",
    "local.prod": "cross-env ROAKIT_ENV=production remix dev --manual",
    "deploy": "npm run build && firebase use default; firebase deploy --only hosting,functions:remixApp,firestore:indexes,firestore:rules",
    "deploy.prod": "npm run build && firebase use prod; firebase deploy --only hosting,functions:remixApp,firestore:indexes,firestore:rules",
    "start": "remix-serve ./functions/build/index.js",
    "tsc": "tsc",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "jest",
    "svgr": "npx @svgr/cli --out-dir app/icons -- icons"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/server": "^11.11.0",
    "@google-cloud/secret-manager": "^5.3.0",
    "@google-cloud/vertexai": "^0.5.0",
    "@mui/material": "^5.15.15",
    "@mui/x-charts": "^7.3.0",
    "@mui/x-data-grid": "^7.3.0",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/server-runtime": "^2.8.1",
    "@uidotdev/usehooks": "^2.4.1",
    "async-retry": "^1.3.3",
    "date-fns": "^3.6.0",
    "fast-memoize": "^2.5.2",
    "firebase": "^10.11.0",
    "firebase-admin": "^12.1.0",
    "google-protobuf": "^3.21.2",
    "markdown-to-jsx": "^7.4.7",
    "material-ui-confirm": "^3.0.12",
    "node-cache": "^5.1.2",
    "pino": "^8.20.0",
    "pluralize": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-linkify-it": "^1.0.8",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@mui/icons-material": "^5.15.15",
    "@remix-run/dev": "^2.8.1",
    "@stylistic/eslint-plugin": "^1.7.2",
    "@svgr/cli": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@testing-library/react": "^14.3.1",
    "@types/async-retry": "^1.4.8",
    "@types/google-protobuf": "^3.15.12",
    "@types/jest": "^29.5.12",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5",
    "vite-tsconfig-paths": "^4.3.2"
