{
  "name": "@roakit/remix-app",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "artifactregistry:login": "npx google-artifactregistry-auth",
    "build": "remix build",
    "build.vite": "remix vite:build",
    "local": "cross-env remix dev --manual",
    "local.vite": "cross-env remix vite:dev --manual",
    "local.prod": "cross-env ROAKIT_ENV=production remix dev --manual",
    "deploy": "npm run build && firebase use default; firebase deploy --only hosting,functions:remixApp",
    "deploy.prod": "npm run build && firebase use prod; firebase deploy --only hosting,functions:remixApp",
    "start": "remix-serve ./functions/build/index.js",
    "typecheck": "tsc",
    "lint": "eslint --ext .js,.ts .",
    "svgr": "npx @svgr/cli --out-dir app/icons -- icons"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@google-cloud/secret-manager": "^5.1.0",
    "@mui/material": "^5.15.12",
    "@mui/x-charts": "^7.0.0-beta.5",
    "@mui/x-data-grid": "^6.19.6",
    "@remix-run/css-bundle": "^2.8.0",
    "@remix-run/node": "^2.8.0",
    "@remix-run/react": "^2.8.0",
    "@remix-run/serve": "^2.8.0",
    "@remix-run/server-runtime": "^2.8.0",
    "date-fns": "^3.3.1",
    "fast-memoize": "^2.5.2",
    "firebase": "^10.8.1",
    "firebase-admin": "^12.0.0",
    "google-protobuf": "^3.21.2",
    "material-ui-confirm": "^3.0.11",
    "pino": "^8.19.0",
    "pluralize": "^8.0.0",
    "react-dom": "^18.2.0",
    "react-linkify-it": "^1.0.8",
    "remix-utils": "^7.5.0",
    "use-local-storage-state": "^19.1.0",
    "use-previous": "^1.2.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@mui/icons-material": "^5.15.12",
    "@remix-run/dev": "^2.8.0",
    "@stylistic/eslint-plugin": "^1.6.3",
    "@svgr/cli": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2"
  },
  "engines": {
    "node": "20"
  }
}
